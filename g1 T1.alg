algoritmo "exe 1"
var
   lc,cc,ar,wh:real
inicio
      escreva ("digite a largura do comodo")
      leia (lc)
      escreva ("digite o comprimento do comodo")
      leia (cc)
      ar <- (cc * lc)
      wh <- (ar * 12)
      escreval(" O comodo contem umaarea de ", ar , "M2")
      escreval("A potencia devera ser de ",wh,"w")
fimalgoritmo